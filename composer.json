{
    "name": "opis-colibri/permissions-sql",
    "type": "opis-colibri-module",
    "description": "SQL-based implementation for opis-colibri/permissions module",
    "keywords": ["opis", "colibri", "module", "permissions module", "sql"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "opis-colibri/permissions": "1.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Colibri\\Modules\\PermissionsSQL\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "module": {
            "title": "User SQL",
            "collector": "Opis\\Colibri\\Modules\\PermissionsSQL\\Collector",
            "installer": "Opis\\Colibri\\Modules\\PermissionsSQL\\Installer"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
